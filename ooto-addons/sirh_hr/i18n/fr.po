msgid ""
"<div>\n"
"          Hi folks,\n"
"          <br/>\n"
"          <br/>\n"
"          Some news update are available on:\n"
"          <br/>\n"
"          <br/>\n"
"          % if object.newsletter:\n"
"          <strong>\n"
"            <span style=\"margin-left:5px\">NEWSLETTER</span>: ${object.base_url}/check/newsletter\n"
