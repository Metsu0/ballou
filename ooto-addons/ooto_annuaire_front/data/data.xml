<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <!--Add menu Annuaire in website-->
    <record id="menu_annuaire_front" model="website.menu">
      <field name="name">Annuaire</field>
      <field name="url">/my/annuaire</field>
      <field name="parent_id" ref="website.main_menu"/>
      <field name="sequence" type="int">5</field>
      <field name="group_ids" eval="[(4, ref('base.group_portal')), (4, ref('base.group_user'))]"/>
    </record>
  </data>
</odoo>