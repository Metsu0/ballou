<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <template id="assets_backend" inherit_id="web.assets_backend">
      <xpath expr="." position="inside">
        <script src="/ooto_onboarding/static/src/js/form_controller.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/form_render.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/basic_fields.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/basic_controller.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/button_render.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/systray_activity_menu.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/reminder.js" type="text/javascript"></script>
        <script src="/ooto_onboarding/static/src/js/fields_section_one2many.js" type="text/javascript"></script>
        <link rel="stylesheet" type="text/scss" href="/ooto_onboarding/static/src/scss/ooto_onboarding.scss"/>
        <link rel="stylesheet" type="text/css" href="/ooto_onboarding/static/src/css/ooto_onboarding.css"/>
        <!-- KANBAN -->
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/kanban_record.js"/>
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/kanban_controller.js"/>
        <!---->
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/mail_message.js"/>
        <!-- DashBoard-->
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/hr_dashboard.js"/>
        <script type="text/javascript" src="/ooto_onboarding/static/lib/charts/Chart.min.js"/>
        <script type="text/javascript" src="/ooto_onboarding/static/lib/charts/Chart.bundle.min.js"/>
        <!-- Css scripts for dashboard view and table -->
        <link rel="stylesheet" href="/ooto_onboarding/static/src/css/dashboard.css"/>
      </xpath>
    </template>
	
    <template id="assets_common" inherit_id="web.assets_common">
      <xpath expr="script[last()]" position="after">
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/sign.js"></script>
      </xpath>
    </template>

    <template id="assets_onboarding_js" inherit_id="web.assets_frontend">
      <xpath expr="." position="inside">
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/my_account.js"></script>
        <script type="text/javascript" src="/ooto_onboarding/static/src/js/fr.js"></script>
      </xpath>
    </template>
  </data>
</odoo>