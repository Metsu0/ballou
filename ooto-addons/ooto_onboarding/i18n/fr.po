"                  %if employee_ids:\n"
"                    % for employee_id in employee_ids:\n"
"                        <tr>\n"
"                          <td width=\"75\">\n"
"                            <img src=\"/web/image/hr.employee/${employee_id.id}/image_128\" alt=\"Avatar\" style=\"vertical-align:baseline; width: 64px; height: 64px; object-fit: cover;\"/>\n"
"                          </td>\n"
"                          <td>\n"
"                            <strong>${employee_id.name}</strong>\n"
"                            <br/>\n"
"                            <span>Post: ${employee_id.job_id.name or ''}</span>\n"
"                          </td>\n"
"                      </tr>\n"
"                    % endfor\n"
"                  %endif\n"
"                </table>\n"
"              </div>\n"
"            </td>\n"
"          </tr>\n"
"        </table>\n"
"      "
msgstr ""
"<table width=\"600\" border=\"1\" cellpadding=\"0\" cellspacing=\"0\" bgcolor=\"#fff\" style=\"border:#f2f2f2 1px solid;\">\n"
