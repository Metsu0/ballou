      <field name="name">Compte de résultat</field>
      <field name="analytic" eval="True"/>
      <field name="unfold_all_filter" eval="True"/>
      <field name="show_journal_filter" eval="True"/>
      <field name='parent_id' ref='pcec_mg_reports.account_reports_mg_menu'/>
    </record>

    <record id="account_financial_report_pcec_is_pr" model="account.financial.html.report.line">
